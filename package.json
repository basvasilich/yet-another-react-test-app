{
  "name": "yet-another-react-test-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "empty": "^0.10.1",
    "enzyme-adapter-react-16": "^1.7.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "prop-types-extra": "^1.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "react-slick": "^0.23.2",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-react": "^7.12.2",
    "flow-bin": "^0.89.0",
    "flow-typed": "^2.5.1",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "flow": "flow",
    "prettier": "prettier --config ./prettier.config.js  --write \"src/**/*.js\"\n"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:flowtype/recommended",
      "react-app"
    ],
    "plugins": [
      "flowtype"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "classes": true,
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
